#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_asar@0.13.0@asar/bin/asar.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_asar@0.13.0@asar/bin/asar.js" "$@"
  ret=$?
fi
exit $ret
